{
    "name": "konecnyjakub/container",
    "description": "A simple PSR-11 container",
    "license": "BSD-3-Clause",
    "keywords": [
        "container",
        "simple",
        "psr-11"
    ],
    "authors": [
        {
            "name": "Jakub Konečný",
            "email": "jakub.konecny2@centrum.cz",
            "role": "Developer"
        }
    ],
    "support": {
        "sources": "https://gitlab.com/konecnyjakub/container",
        "issues": "https://gitlab.com/konecnyjakub/container/-/issues"
    },
    "require": {
        "php": ">=8.3.0",
        "psr/container": "^1.1 || ^2.0"
    },
    "require-dev": {
        "php-parallel-lint/php-parallel-lint": "^1.4",
        "php-parallel-lint/php-console-highlighter": "^0.5",
        "squizlabs/php_codesniffer": "^3.13",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "konecnyjakub/mytester": "^8.2"
    },
    "provide": {
        "psr/container-implementation": "1.0 | 2.0"
    },
    "autoload": {
        "psr-4": {
            "Konecnyjakub\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konecnyjakub\\Container\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
